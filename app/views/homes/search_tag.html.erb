<div class="container">
  <%= render 'homes/search', menber:@menbers, post:@posts, tag:@tag %>  <%#  部分テンプレート  render 'homes/search'～  homes/_search  _は勝手に消してくれる%>
  <div class="row">
    <div class="col-12-flex flex-lg-wrap">
      <h4 class="title">検索結果</h4>
    </div>
  </div>
  <div class="row flex-nowrap">
    <% @tag.posts.each do |tag_post| %>
      <div class="col-3">
        <%= link_to post_path(tag_post.id) do %>  <%# link_to　が効かない%>
          <%= attachment_image_tag tag_post.post_images.first, :image, :fill, 150, 150, fallback:'no_image.jpeg', size: "200x200" %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>