<h2>マイページ</h2>
  <div class="container">
    <div class="row">
      <div class="col_lg_7">
        <table>
          <thead>
            <tr>
              <th>アイコン</th>
              <th><%= attachment_image_tag @menber, :image, :fill, 100, 100 %></th>
            </tr>
            <tr>
              <th>ニックネーム</th>
              <th><%= @menber.name %></th>
            </tr>
            <tr>
              <th>紹介文</th>
              <th><%= @menber.intorduction %></th>
            </tr>
          </thead>
        </table>
        <p><%= link_to "個人設定", edit_menber_path(current_menber) %></p>
      </div>
    </div>
    <div class="row">
      <div class="col-12-flex flex-lg-wrap" >
        <% @posts.each do |post| %>
          <%= link_to(post_path(post), class: 'list-content') do %>
            <p><%= attachment_image_tag post, :image_id, :fill, 100, 100 %> </p>
          <% end %>
        <% end %>
      </div>
    </div>
  </div>
